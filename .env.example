# API Keys
# Get your Google Safe Browsing API key from: https://console.cloud.google.com/apis/library/safebrowsing.googleapis.com
SAFE_BROWSING_API_KEY=your_safe_browsing_api_key_here

# Get your VirusTotal API key from: https://www.virustotal.com/gui/join-us
VIRUSTOTAL_API_KEY=your_virustotal_api_key_here

# Server Configuration
PORT=3000
NODE_ENV=development

# Security
CORS_ORIGIN=http://localhost:3000
RATE_LIMIT_WINDOW_MS=900000  # 15 minutes
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE=logs/app.log

# Cache Configuration
CACHE_TTL=86400  # 24 hours in seconds
MAX_CACHE_SIZE=1000  # Maximum number of cached URLs

# Model Configuration
MODEL_PATH=./models/phishing_detector.h5
CONFIDENCE_THRESHOLD=0.8

# Email Configuration
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your_smtp_user
SMTP_PASS=your_smtp_password 